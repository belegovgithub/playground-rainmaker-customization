{
  "key": "newpt-receipt",
  "config": {
    "defaultStyle": {
      "font": "Camby"
    },
    "watermark": {
      "text": "BEL UAT BEL UAT",
      "color": "gery",
      "opacity": 0.1,
      "bold": true,
      "italics": false
    },
