      {
        "style": "ws-head",
        "table": {
          "widths": ["*"],
          "body": [
            [
              {
                "columns": [
                  {
                    "stack": [
                      {
                        "text": "{{cb-name}} {{cantonment-board}}",
                        "style": "pdf-title"
                      },
                      {
                        "text": "{{form-no}}",
                        "style": "pdf-title"
                      },
                      {
                        "text": "{{receipt_voucher_label}}",
                        "style": "pdf-title"
                      },
                      {
                        "text": "{{rule_label}}",
                        "style": "pdf-title"
                      }
                    ]
                  }
                ]
              }
            ]
          ]
        },
        "layout": "noBorders",
        "margin": [0, 20, 0, 20]
      },
      {
        "style": "ws-head",
        "table": {
          "widths": ["15%", "85%"],
          "body": [
            [
              {
                "text": "{{receipt_no_label}}",
                "style": "pdf-label",
