              {
                "stack": [
                  {
                    "text": "............................."
                  },
                  {
                    "text": "{{signature_label}}"
                  },
                  {
                    "text": "{{designation_label}}"
                  }
                ],
                "border": [false, false, true, true],
                "borderColor": ["#A9A9A9", "#A9A9A9", "#A9A9A9", "#A9A9A9"]
              }
            ]
          ]
        }
      }
