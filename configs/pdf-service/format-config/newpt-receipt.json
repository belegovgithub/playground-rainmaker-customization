              {
                "text": "{{total}}",
                "style": "pdf-label",
                "borderColor": ["#A9A9A9", "#A9A9A9", "#A9A9A9", "#A9A9A9"]
              }
            ]
          ]
        }
      },
      {
        "style": "ws-head",
        "table": {
          "widths": ["70%", "30%"],
          "heights": [40, 50],
          "body": [
            [
              {
                "text": "{{disclaimer}}",
                "border": [true, false, false, false],
                "borderColor": ["#A9A9A9", "#A9A9A9", "#A9A9A9", "#A9A9A9"]
              },
              {
                "text": " ",
                "border": [false, false, true, false],
                "borderColor": ["#A9A9A9", "#A9A9A9", "#A9A9A9", "#A9A9A9"]
              }
            ],
            [
              {
                "text": " ",
                "border": [true, false, false, true],
                "borderColor": ["#A9A9A9", "#A9A9A9", "#A9A9A9", "#A9A9A9"]
