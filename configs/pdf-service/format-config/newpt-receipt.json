    "styles": {
      "pdf-title": {
        "alignment": "center"
      },
      "pdf-label": {
        "bold": true,
        "fillColor": "#E6E6FA",
        "border": [true, true, true, true]
      },
      "pdf-value": {
        "border": [true, true, true, true]
